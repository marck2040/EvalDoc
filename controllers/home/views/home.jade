extends ../../../views/layout
block content
	div.row-fluid
		div.col-md-12
			div.row-fluid
				div.col-md-1
					div.center
						img(src='images/univo.jpg' width='100')
				div.col-md-11
					table.table.table-striped.table-bordered.table-hover
						thead
							tr
								th
									| Materias	
								th
									| Docentes
						tbody
							each materia, i in materias
								tr
									td 
										i.ace-icon.fa.fa-edit.blue
										a(href= '/eval/#{materia.mat_codigo}-#{materia.emp_codigo}') #{materia.mat_nombre}
									td 
										i.ace-icon.fa.fa-edit.blue
										a(href='/eval/#{materia.mat_codigo}-#{materia.emp_codigo}') #{materia.emp_apellidos_nombres}

	